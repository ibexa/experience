{
    "name": "ibexa/experience",
    "type": "metapackage",
    "license": "proprietary",
    "description": "A meta package for installing Ibexa DXP Experience edition",
    "repositories": [
        {"type": "composer", "url": "https://updates.ibexa.co"}
    ],
    "minimum-stability": "RC",
    "prefer-stable": true,
    "require": {
        "php": "^7.4 || ^8.0",
        "ibexa/content": "~4.0.0@rc",
        "ibexa/form-builder": "~4.0.0@rc",
        "ibexa/page-builder": "~4.0.0@rc",
        "ibexa/fieldtype-page": "~4.0.0@rc",
        "ibexa/permissions": "~4.0.0@rc",
        "ibexa/segmentation": "~4.0.0@rc",
        "ibexa/site-factory": "~4.0.0@rc"
    },
    "require-dev": {
        "ibexa/ci-scripts": "^0.2@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
