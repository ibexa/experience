{
    "name": "ibexa/experience",
    "type": "metapackage",
    "license": "proprietary",
    "description": "A meta package for installing Ibexa DXP Experience edition",
    "repositories": [
        {
            "type": "composer",
            "url": "https://updates.ibexa.co"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ibexa/content": "4.5.2",
        "ibexa/form-builder": "4.5.2",
        "ibexa/page-builder": "4.5.2",
        "ibexa/fieldtype-address": "4.5.2",
        "ibexa/fieldtype-page": "4.5.2",
        "ibexa/permissions": "4.5.2",
        "ibexa/segmentation": "4.5.2",
        "ibexa/site-factory": "4.5.2",
        "ibexa/corporate-account": "4.5.2"
    },
    "require-dev": {
        "ibexa/ci-scripts": "^0.2@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.5.x-dev"
        }
    }
}
