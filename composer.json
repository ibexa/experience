{
    "name": "ibexa/experience",
    "type": "metapackage",
    "license": "proprietary",
    "description": "A meta package for installing Ibexa DXP Experience edition",
    "repositories": [
        {"type": "composer", "url": "https://updates.ibexa.co"}
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "ibexa/content": "^4.0.x-dev",
        "ibexa/form-builder": "^4.0.x-dev",
        "ibexa/page-builder": "^4.0.x-dev",
        "ibexa/fieldtype-page": "^4.0.x-dev",
        "ibexa/permissions": "^4.0.x-dev",
        "ibexa/segmentation": "^4.0.x-dev",
        "ibexa/site-factory": "^4.0.x-dev"
    },
    "require-dev": {
        "ibexa/ci-scripts": "dev-debug-flex-server as 0.2.x-dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
