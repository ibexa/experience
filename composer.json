{
    "name": "ibexa/experience",
    "type": "metapackage",
    "license": "proprietary",
    "description": "A meta package for installing Ibexa DXP Experience edition",
    "repositories": [
        {
            "type": "composer",
            "url": "https://updates.ibexa.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ibexa/activity-log": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/headless": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/form-builder": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/page-builder": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/fieldtype-address": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/fieldtype-page": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/dashboard": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/permissions": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/segmentation": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/site-factory": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/corporate-account": "dev-ibx-8471-symfony-7 as 5.0.x-dev",
        "ibexa/engage": "dev-ibx-8471-symfony-7 as 5.0.x-dev"
    },
    "require-dev": {
        "ibexa/ci-scripts": "^0.2@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    }
}
